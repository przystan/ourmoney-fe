{
  "name": "ourmoney-fe",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "prettier-tslint fix",
      "git add"
    ],
    "src/**/*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.*css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "ng-mocks": "^10.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@ngneat/spectator": "^5.11.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "protractor": "~7.0.0",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
